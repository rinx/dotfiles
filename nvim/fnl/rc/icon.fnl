;; ; => "â ±"
;; (pos->braille [0 0]
;;               [1 1]
;;               [1 2])
(fn pos->braille [...]
  (let [->idx (fn [[x y]]
                (* (^ 2 y) (^ 16 x)))]
                ;; returns an index to enable specified place (x, y) of braille.
                ;; x: [0-1], y: [0-3]"
    (->> [...]
         (core.map ->idx)
         (core.reduce (fn [acc x]
                        (+ acc x)) 0)
         (core.inc)
         (. brailles))))

